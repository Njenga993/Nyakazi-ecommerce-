{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "./src",
    "types": ["react", "react-dom", "react-router-dom"],
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
